datasource db {
  provider = "sqlite"
  url      = "file:database.db"
}

generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
}

model Stock {
  id        Int      @id @default(autoincrement())
  symbol    String
  name      String
  price     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}